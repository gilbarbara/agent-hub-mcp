{
  "name": "agent-hub-mcp",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@gilbarbara/eslint-config": "^0.8.8",
    "@gilbarbara/prettier-config": "^1.0.0",
    "@gilbarbara/tsconfig": "^0.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.2",
    "@vitest/coverage-v8": "^3.2.4",
    "del-cli": "^6.0.0",
    "husky": "^9.1.7",
    "is-ci-cli": "^2.2.0",
    "jest-extended": "^6.0.0",
    "repo-tools": "^0.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "watch": "tsup --watch",
    "clean": "del dist/*",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:sse": "tsx src/sse-server.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint --fix src",
    "typecheck": "tsc",
    "typevalidation": "attw -P",
    "format": "prettier \"**/*.{css,graphql,js,json,jsx,less,md,mdx,scss,ts,tsx,yaml,yml}\" --write",
    "size": "size-limit",
    "validate": "npm run lint && npm run typecheck && npm run test:coverage && npm run build && npm run typevalidation",
    "prepare": "husky",
    "prepublishOnly": "npm run validate"
  },
  "tsup": {
    "dts": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "splitting": false
  },
  "eslintConfig": {
    "extends": [
      "@gilbarbara/eslint-config/base",
      "@gilbarbara/eslint-config/vitest"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "no-await-in-loop": "off",
      "no-continue": "off",
      "no-empty-function": "off"
    }
  },
  "prettier": "@gilbarbara/prettier-config",
  "size-limit": [
    {
      "name": "commonjs",
      "path": "./dist/index.js",
      "limit": "15 kB"
    },
    {
      "name": "esm",
      "path": "./dist/index.mjs",
      "limit": "15 kB"
    }
  ]
}
